                                        <fileTypes>
                                                <filetype name="Fasta" extension="fa" description="FASTA Sequence Format" />
                                                <filetype name="Fasta" extension="fasta" description="BLAST input query FASTA file" />
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="MosaikBuild_1.Outputfileforreference" name="Output file for reference" required="false" enabled="false" order="5" prefix="-oa" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="DAT file" extension="dat" description="A DAT type file" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="MosaikBuild_1.colorspaceflag" name="colorspace flag" description="Translates the reference sequence from basespace to colorspace." required="false" enabled="false" order="3" prefix="-cs" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="MosaikBuild_1.Inputforwardsequence" name="Input forward sequence " required="false" enabled="true" order="1" prefix="-q" prefixSpaced="true" prefixAllArgs="false">
