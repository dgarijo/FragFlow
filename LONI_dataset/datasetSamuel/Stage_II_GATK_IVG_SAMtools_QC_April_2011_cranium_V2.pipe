                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKRealigning_1.BAMInputFile" name="BAM Input File" description="/path/to/input.bam" required="true" enabled="true" order="2" switch="-I" switchSpaced="true">
                                <format type="File" cardinality="1">
