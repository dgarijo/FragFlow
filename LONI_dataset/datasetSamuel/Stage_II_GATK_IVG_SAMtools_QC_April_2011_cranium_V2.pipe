                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKRealigning_1.TargetIntervals" name="Target Intervals" description="Interval List Output from &quot;Creating Intervals&quot; module" required="true" enabled="true" order="5" switch="-targetIntervals" switchSpaced="true">
