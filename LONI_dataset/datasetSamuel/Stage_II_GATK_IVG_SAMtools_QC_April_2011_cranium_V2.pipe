                                        <fileTypes>
                                                <filetype name="File" extension="" description="Any type of data file" />
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKRealigning_1.DBSNPCatalogSNP" name="DBSNP Catalog SNP" description="/path/to/dbsnp.rod&#xA;&#xA;known snps  location " required="false" enabled="true" order="8" switch="-D" switchSpaced="true">
