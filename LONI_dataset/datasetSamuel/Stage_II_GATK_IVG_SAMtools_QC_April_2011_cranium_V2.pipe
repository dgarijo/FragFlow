                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKRealigning_1.GATKIndelRealigner" name="GATK IndelRealigner" description="IndelRealigner" required="true" enabled="true" order="4" switch="-T IndelRealigner" switchSpaced="true">
