                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="SAMtools BAM file" extension="bam" description="SAMtools BAM file" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="GATKRealigning_1.Indelsbinding" name="Indels binding" description="indels,VCF /path/to/indel_calls.vcf&#xA;&#xA;indels and dbsnp bindings would be used to pass in known indel sites for the realigner to target." required="false" enabled="false" order="7" switch="-B" switchSpaced="true">
