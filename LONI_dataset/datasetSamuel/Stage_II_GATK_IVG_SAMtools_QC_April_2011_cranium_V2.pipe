                        <input id="GATKCovariatescalculation_1.Jar" name="Jar" description="Application_Jar " required="true" enabled="true" order="0" switch="-jar" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="File" extension="" description="Any type of data file" />
                                        </fileTypes>
                                </format>
                                <values>
                                        <value>pipeline://cranium.loni.ucla.edu/{GATK}/GenomeAnalysisTK.jar</value>
                                </values>
                        </input>
                        <input id="GATKCovariatescalculation_1.FASTAInputFile" name="FASTA Input File" description="Reference Genome input file (FASTA format)" required="true" enabled="true" order="2" switch="-R" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Fasta" extension="fa" description="FASTA Sequence Format" />
                                                <filetype name="Fasta" extension="fasta" description="BLAST input query FASTA file" />
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="GATKCovariatescalculation_1.GATKCovariateCatalogListOutputFile" name="GATK Covariate Catalog List Output File" description="A output file" required="true" enabled="true" order="10" switch="-recalFile" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="csv file" extension="csv" description="excel delimited file" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="GATKCovariatescalculation_1.DBSNPSNPcatalog" name="DBSNP SNP catalog" description="DBSNP" required="true" enabled="true" order="3" switch="--DBSNP" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="rod" extension="rod" description="DBSNP GATK Catalog of NCBI SNPs" />
                                                <filetype name="vfc" extension="vfc" description="DBSNP New GATK SNP format" />
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKCovariatescalculation_1.BAMInputFile" name="BAM Input File" required="true" enabled="true" order="4" switch="-I" switchSpaced="true">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="SAMtools BAM file" extension="bam" description="SAMtools BAM file" />
                                        </fileTypes>
                                </format>
                        </input>
                        <input id="GATKCovariatescalculation_1.GATKWalkerforCovariateCount" name="GATK Walker for Covariate Count" description="CountCovariates" required="true" enabled="true" order="5" switch="-T CountCovariates" switchSpaced="true">
                                <format type="String" cardinality="0" />
                                <dependencies>
                                        <dependent>Original qualities scores (OQ Attribute)</dependent>
                                        <dependent>SOLiD Recalibration Mode</dependent>
                                        <dependent>Sites Skip</dependent>
                                        <dependent>MNQS Window Size</dependent>
                                        <dependent>Parallel CountCovariates</dependent>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.GATKCovariateCountControl1" name="GATK Covariate Count Control 1" required="true" enabled="true" order="6" switch="-cov" switchSpaced="true">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="GATKCovariatescalculation_1.PlatformReadTypeSpecsFlags" name="Platform Read Type Specs Flags" required="false" enabled="true" order="16" switch="--default_platform" switchSpaced="true">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="GATKCovariatescalculation_1.InfoFlag" name="Info Flag" required="true" enabled="true" order="1" switch="-l" switchSpaced="true">
                                <format type="String" cardinality="1" />
                                <values>
                                        <value>INFO</value>
                                </values>
                        </input>
                        <input id="GATKCovariatescalculation_1.GATKCovariateCountControl2" name="GATK Covariate Count Control 2" required="true" enabled="true" order="7" switch="-cov" switchSpaced="true">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="GATKCovariatescalculation_1.GATKCovariateCountControl3" name="GATK Covariate Count Control 3" required="true" enabled="true" order="8" switch="-cov" switchSpaced="true">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="GATKCovariatescalculation_1.GATKCovariateCountControl4" name="GATK Covariate Count Control 4" required="true" enabled="true" order="9" switch="-cov" switchSpaced="true">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="GATKCovariatescalculation_1.OriginalqualitiesscoresOQAttribute" name="Original qualities scores (OQ Attribute)" description="takes the base quality scores from the OQ attribute field and not the QUALs field. " required="false" enabled="false" order="11" switch="-OQ" switchSpaced="true">
                                <format type="String" cardinality="0" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.SOLiDRecalibrationMode" name="SOLiD Recalibration Mode" description="governs how the recalibrator handles the reads which have had the reference inserted because of color space inconsistencies. The three implemented modes are:&#xA;SET_Q_ZERO - Set reference inserted bases and the previous base (because of color space alignment details) to Q0.&#xA;SET_Q_ZERO_BASE_N - In addition to setting the quality scores to zero, also set the base itself to 'N'. This is useful to visualize in IGV.&#xA;REMOVE_REF_BIAS - Look at the color quality scores and probabilistically decide to change the reference inserted base to be the base which is implied by the original color space instead of the reference." required="false" enabled="false" order="12" switch="--solid_recal_mode" switchSpaced="true">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>SET_Q_ZERO</enumeration>
                                        <enumeration>SET_Q_ZERO_BASE_N</enumeration>
                                        <enumeration>REMOVE_REF_BIAS</enumeration>
                                </format>
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.SitesSkip" name="Sites Skip" description="CountCovariates accepts any rod file of type VariantRod as a list of sites to skip over, including vcf files. Activate this feature using the -B:name,type filename argument. For example, -B:mask,VCF path_to/my_snp_calls.vcf" required="false" enabled="false" order="13" switch="-B" switchSpaced="false">
                                <format type="String" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                                <values>
                                        <value>mask,VCF path_to/my_snp_calls.vcf</value>
                                </values>
                        </input>
                        <input id="GATKCovariatescalculation_1.MNQSWindowSize" name="MNQS Window Size" description="Passed to the MinimumNQSCovariate to specify the window size that should be used. This window size tells the module in how big of a neighborhood around the current base it should look for the minimum base quality score." required="false" enabled="false" order="14" switch="-nqs" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.ParallelCountCovariates" name="Parallel CountCovariates" description="CountCovariates can be run with shared memory parallelism using the -nt option. For example, -nt 10 will run CountCovariates with 10 parallel thread." required="false" enabled="false" order="15" switch="-nt" switchSpaced="true">
                                <format type="Number" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <output id="GATKCovariatescalculation_1.OutputFlowcontrol" name="Output Flow control" description="Dummy parameter for flow control" required="false" enabled="false" order="17">
                                <format type="Flow Control" cardinality="1" />
                        </output>
                </module>
                <module id="GATKTableRecalibration_1" name="GATK Table Recalibration" package="GATK" description="http://www.broadinstitute.org/gsa/wiki/index.php/Base_quality_score_recalibration&#xA;&#xA;java -Xmx4g -jar GenomeAnalysisTK.jar \&#xA;  -l INFO \  &#xA;  -R resources/Homo_sapiens_assembly18.fasta \&#xA;  -I my_reads.bam \&#xA;  -T TableRecalibration \&#xA;   -outputBam my_reads.recal.bam \&#xA;   -recalFile my_reads.recal_data.csv&#xA;&#xA;II.a.3 Recalibration (http://www.broadinstitute.org/gsa/wiki/index.php/Base_quality_score_recalibration, section Table recalibration)&#xA;Input: alignment.rmd.sorted.md.bam, reference.fasta file, recal_1.csv Tool: GATK (GenomeAnalysisTK.jar, -T TableRecalibration)&#xA;Server Location: /applications/gatk/GATK-2010-01-14/dist&#xA;Output:alignment.rmd.sorted.md.recal.bam&#xA; &#xA;Example: nohup java -Xmx4g -jar GenomeAnalysisTK.jar -l INFO -R /projects/USC/production/ref/ensembl_ncbi36/ensembl_hg18_ncbi36_r50.fa -I /projects1/ADNI_2/BWA/Input/s_1_1_sequence.s_1_2_sequence.mapped.rmd.sorted.md.bam -T TableRecalibration --out /projects1/ADNI_2/BWA/Input/s_1_1_sequence.s_1_2_sequence.mapped.rmd.sorted.md.recal.bam -recalFile JLK-227.recal.csv - -default_platform illumina&#xA;&#xA;This module uses the recalibration table data in my_reads.recal_data.csv produced by CountCovariates to recalibrate the quality scores in my_reads.bam, writing out a new BAM file my_reads.recal.bam with recalibrated QUAL field values." location="pipeline://cranium.loni.ucla.edu//usr/local/java/bin/java" sourceCode="" icon="/9j/2wBDAAgGBgcGBQgHBwcJCQgKDBQNDAsLDBkSEw8UHRofHh0aHBwgJC4nICIsIxwcKDcpLDAx&#xA;NDQ0Hyc5PTgyPC4zNDL/2wBDAQkJCQwLDBgNDRgyIRwhMjIyMjIyMjIyMjIyMjIyMjIyMjIyMjIy&#xA;MjIyMjIyMjIyMjIyMjIyMjIyMjIyMjIyMjL/wAAUCAA8AFYEASIAAhEBAxEBBCIA/8QAHwAAAQUB&#xA;AQEBAQEAAAAAAAAAAAECAwQFBgcICQoL/8QAtRAAAgEDAwIEAwUFBAQAAAF9AQIDAAQRBRIhMUEG&#xA;E1FhByJxFDKBkaEII0KxwRVS0fAkM2JyggkKFhcYGRolJicoKSo0NTY3ODk6Q0RFRkdISUpTVFVW&#xA;V1hZWmNkZWZnaGlqc3R1dnd4eX