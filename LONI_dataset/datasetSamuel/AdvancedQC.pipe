                        <input id="IntervalCreation_1.FASTAInputFile" name="FASTA Input File" description="Reference Genome input file (FASTA format)" required="true" enabled="true" order="3" prefix="-R" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="Fasta" extension="fa" description="FASTA Sequence Format" />
