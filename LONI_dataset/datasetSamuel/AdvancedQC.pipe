                                </values>
                        </input>
                        <input id="GATKCovariatescalculation_1.FASTAInputFile" name="FASTA Input File" description="Reference Genome input file (FASTA format)" required="true" enabled="true" order="2" prefix="-R" prefixSpaced="true" prefixAllArgs="false">
