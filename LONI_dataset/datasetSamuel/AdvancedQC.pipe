                        <output id="MarkDuplicates_1.MetricsFile" name="Metrics File" description="File to write duplication metrics to Required." required="true" enabled="true" order="12" prefix="METRICS_FILE=" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="1">
                                        <fileTypes>
