                        <input id="GATKCovariatescalculation_1.OriginalqualitiesscoresOQAttribute" name="Original qualities scores (OQ Attribute)" description="takes the base quality scores from the OQ attribute field and not the QUALs field. " required="false" enabled="false" order="11" prefix="-OQ" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.SOLiDRecalibrationMode" name="SOLiD Recalibration Mode" description="governs how the recalibrator handles the reads which have had the reference inserted because of color space inconsistencies. The three implemented modes are:&#xA;SET_Q_ZERO - Set reference inserted bases and the previous base (because of color space alignment details) to Q0.&#xA;SET_Q_ZERO_BASE_N - In addition to setting the quality scores to zero, also set the base itself to 'N'. This is useful to visualize in IGV.&#xA;REMOVE_REF_BIAS - Look at the color quality scores and probabilistically decide to change the reference inserted base to be the base which is implied by the original color space instead of the reference." required="false" enabled="false" order="12" prefix="--solid_recal_mode" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Enumerated" cardinality="1">
                                        <enumeration>SET_Q_ZERO</enumeration>
                                        <enumeration>SET_Q_ZERO_BASE_N</enumeration>
                                        <enumeration>REMOVE_REF_BIAS</enumeration>
                                </format>
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.SitesSkip" name="Sites Skip" description="CountCovariates accepts any rod file of type VariantRod as a list of sites to skip over, including vcf files. Activate this feature using the -B:name,type filename argument. For example, -B:mask,VCF path_to/my_snp_calls.vcf" required="false" enabled="false" order="13" prefix="-B" prefixSpaced="false" prefixAllArgs="false">
                                <format type="String" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                                <values>
                                        <value>mask,VCF path_to/my_snp_calls.vcf</value>
                                </values>
                        </input>
                        <input id="GATKCovariatescalculation_1.MNQSWindowSize" name="MNQS Window Size" description="Passed to the MinimumNQSCovariate to specify the window size that should be used. This window size tells the module in how big of a neighborhood around the current base it should look for the minimum base quality score." required="false" enabled="false" order="14" prefix="-nqs" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
                        <input id="GATKCovariatescalculation_1.ParallelCountCovariates" name="Parallel CountCovariates" description="CountCovariates can be run with shared memory parallelism using the -nt option. For example, -nt 10 will run CountCovariates with 10 parallel thread." required="false" enabled="false" order="15" prefix="-nt" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                                <dependencies>
                                        <dependsOn>GATK Walker for Covariate Count</dependsOn>
                                </dependencies>
                        </input>
