                                        </fileTypes>
                                </format>
                        </output>
                        <input id="IntervalCreation_1.Indelsbinding" name="Indels binding" description="indels,VCF /path/to/indel_calls.vcf&#xA;&#xA;indels and dbsnp bindings would be used to pass in known indel sites for the realigner to target." required="false" enabled="false" order="8" prefix="-B" prefixSpaced="true" prefixAllArgs="false">
