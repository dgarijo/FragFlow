                                                <filetype name="SAMtools BAM file" extension="bam" description="SAMtools BAM file" />
                                        </fileTypes>
                                </format>
                        </input>
                        <output id="MarkDuplicates_1.Output" name="Output" description="The output file to right marked records to Required. " required="true" enabled="true" order="11" prefix="OUTPUT=" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="SAMtools BAM file" extension="bam" description="SAMtools BAM file" />
                                        </fileTypes>
                                </format>
                        </output>
                        <input id="MarkDuplicates_1.JarFile" name="Jar File" required="true" enabled="true" order="0" prefix="-jar" prefixSpaced="true" prefixAllArgs="false">
                                <format type="File" cardinality="1">
                                        <fileTypes>
                                                <filetype name="JAR" extension="jar" description="Sun JAR file" />
