                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.TempDirectory" name="Temp Directory" required="false" enabled="true" order="0" prefix="-Djava.io.tmpdir=" prefixSpaced="false" prefixAllArgs="false">
                                <format type="String" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.LOD" name="LOD" description="--LODThresholdForCleaning; LOD threshold above which the realigner will proceed to realign; default=5.0]&#xA;This term is equivalent to &quot;significance&quot; - i.e. is the improvement significant enough to merit realignment? Note that this number should be adjusted based on your particular data set. For low coverage and/or when looking for indels with low allele frequency, this number should be smaller." required="false" enabled="false" order="11" prefix="-LOD" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.AssumeUnsortedInvertals" name="Assume Unsorted Invertals" description="--targetIntervalsAreNotSorted; This tool assumes that the target interval list is sorted; if the list turns out to be unsorted, it will throw an exception. Use this argument when your interval list is not sorted to instruct the Realigner to first sort it in memory." required="false" enabled="false" order="12" prefix="-targetNotSorted" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="IntervalCreation_1.UseKnownIndels" name="Use Known Indels" description="--useOnlyKnownIndels; Don't run 'Smith-Waterman' to generate alternate consenses; use only known indels provided as RODs for constructing the alternate references." required="false" enabled="false" order="13" prefix="-knownsOnly" prefixSpaced="true" prefixAllArgs="false">
                                <format type="String" cardinality="0" />
                        </input>
                        <input id="IntervalCreation_1.Maxreadsinmemory" name="Max reads in memory" description="Max reads allowed to be kept in memory at a time by the writer. To minimize memory consumption you can lower this number (but then the tool may skip realignment on regions with too much coverage; and if the number is too low, it may generate errors during realignment). Just make sure to give Java enough memory! 4Gb should be enough with the default value of 200,000." required="false" enabled="false" order="14" prefix="--maxReadsInMemory" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.EntropyThreshold" name="Entropy Threshold" description="Percentage of mismatching base quality scores at a position to be considered having high entropy [default=0.15]&#xA;This is similar to the argument in the RealignerTargetCreator walker. The point here is that the realigner will only proceed with the realignment (even above the given threshold) if it minimizes entropy among the reads (and doesn't simply push the mismatch column to another position). This parameter is just a heuristic and should be adjusted based on your particular data set." required="false" enabled="false" order="15" prefix="--EntropyThreshold" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.MaxConsensuses" name="Max Consensuses" description="Max alternate consensuses to try (necessary to improve performance in deep coverage) [default=30]&#xA;If you need to find the optimal solution regardless of running time, use a higher number." required="false" enabled="false" order="16" prefix="--maxConsensuses" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.Maxreadsusedforconsensuses" name="Max reads used for consensuses" description="Max reads (chosen randomly) used for finding the potential alternate consensuses (necessary to improve performance in deep coverage) [default=120]&#xA;If you need to find the optimal solution regardless of running time, use a higher number." required="false" enabled="false" order="17" prefix="--maxReadsForConsensuses" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <input id="IntervalCreation_1.Maxreadsallowedforrealignment" name="Max reads allowed for realignment" description="Max reads allowed at an interval for realignment. If this value is exceeded, realignment is not attempted and the reads are passed to the output file(s) as is [default=20000]&#xA;If you need to allow more reads (e.g. with very deep coverage) regardless of memory, use a higher number." required="false" enabled="false" order="18" prefix="--maxReadsForRealignment" prefixSpaced="true" prefixAllArgs="false">
                                <format type="Number" cardinality="1" />
                        </input>
                        <output id="IntervalCreation_1.OutputIntervalsFile" name="Output Intervals File" description="realignedBam.bam&#xA;" required="true" enabled="true" order="7" prefix="-o" prefixSpaced="true" prefixAllArgs="false">
