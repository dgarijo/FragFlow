                                        <values>
                                                <value>300</value>
                                        </values>
                                </input>
                                <input id="KL_MI_register_4.griddim1" name="grid dim 1" description="pixel number along x direction of the common grid.  If the template and target have the same grid, the default is the same as the input." required="false" enabled="true" order="8" switch="-i" switchSpaced="true">
