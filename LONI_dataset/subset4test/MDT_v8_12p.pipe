                                        <values>
                                                <value>3</value>
                                        </values>
                                </input>
                                <input id="KL_MI_register_4.labmda" name="labmda" description="labmda -- weight of the KL distance term (default = 0.5)" required="false" enabled="false" order="16" switch="-l" switchSpaced="true">
                                        <format type="Number" cardinality="1" />
                                        <values>
