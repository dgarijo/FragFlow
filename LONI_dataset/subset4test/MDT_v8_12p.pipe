                                <input id="KL_MI_register_4.fidelityterm" name="fidelity term " description="fidelity term (1 = L2, 2 = Mutual Information, default = 2)&#xA;3 -- MI with inverse consistency, takes ~twice as long" required="false" enabled="true" order="15" switch="-f" switchSpaced="true">
                                        <format type="Enumerated" cardinality="1">
